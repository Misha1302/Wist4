import "../../../../Libraries/net8.0/StandardLibrary.dll"


foo() -> void (
    WriteI64(222)
    bar()
    ret 0
)

bar() -> void (
    WriteI64(333)
    ret 0
)

main() -> int64 (
    // need to drop foo value. Otherwise we'll catch stack overflow
    for (int64 i = 0) (i < 100000000) (i = i + 1) ( foo() )
	ret 123
)