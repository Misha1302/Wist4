import "../../../../Libraries/net8.0/StandardLibrary.dll"

factorial(int64 x) -> int64 (
    if (x <= 2) ( ret (x) )
    ret x * factorial(x - 1)
)

doubleMul(int64 a, int64 b) -> int64 (
   ret a * b * 2 
)

main() -> int64 (
    for (int64 i = 1) (i <= 20) (i = i + 1) (
        WriteI64NoLn(i)
        WriteI64(factorial(i))
        WriteI64(5 + doubleMul(i, 5 + i))
    ) 
    ret 0
)